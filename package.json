{
  "name": "ipixxel-ai-assistant",
  "version": "1.0.0",
  "description": "AI Assistant Add-in for Microsoft Word and Excel",
  "main": "dist/index.js",
  "scripts": {
    "generate-manifest": "node scripts/generate-manifest.js",
    "dev": "npm run generate-manifest && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "webpack serve --mode development",
    "build": "npm run generate-manifest && webpack --mode production",
    "start": "node server/index.js",
    "start:office": "npm run generate-manifest && office-addin-debugging start manifest.xml",
    "validate": "npm run generate-manifest && office-addin-manifest validate manifest.xml",
    "lint": "eslint src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "openai": "^4.20.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/office-js": "^1.0.441",
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "ts-loader": "^9.5.1",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "office-addin-debugging": "^5.0.0",
    "office-addin-dev-certs": "^1.11.0",
    "office-addin-manifest": "^1.12.0"
  }
}